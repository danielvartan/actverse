citHeader("To cite actstudio in publications use:")

citEntry(
  entry       = "Unpublished",
  title       = paste0("A R package for processing, analyzing and ",
                       "visualizing actigraphy data"),
  author      = personList(as.person("Daniel Vartanian"),
  				                as.person("Bruno Furlan"),
  			                  as.person("Vinicius Matias"),
  			     	            as.person("Cassio Serrano"),
                          as.person("Ana Amelia Benedito-Silva"),
                          as.person("Mario Pedrazzoli")),
  year        = "2021",
  url         = "https://gipsousp.github.io/actstudio/",
  textVersion = paste0("Vartanian D., Furlan, B., Matias, v., Serrano, C., ",
                        "Benedito-Silva, A. A., Pedrazzoli, M. ",
                        "(2021). A R package for processing, analyzing and ",
                        "visualizing actigraphy data. ",
                        "Retrieved from ",
                        "https://gipsousp.github.io/actstudio/."),
  note        = "Lifecycle: experimental"
)
